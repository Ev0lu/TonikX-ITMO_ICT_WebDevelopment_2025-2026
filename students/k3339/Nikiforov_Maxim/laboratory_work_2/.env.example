# PostgreSQL Database Configuration
# Скопируйте этот файл в .env и отредактируйте значения

# Database Engine
DB_ENGINE=django.db.backends.postgresql

# Database Name
DB_NAME=homework_db

# Database User
DB_USER=postgres

# Database Password
DB_PASSWORD=your_secure_password_here

# Database Host
DB_HOST=localhost

# Database Port
DB_PORT=5432

# Django Settings
SECRET_KEY=your_secret_key_here
DEBUG=True
ALLOWED_HOSTS=localhost,127.0.0.1
